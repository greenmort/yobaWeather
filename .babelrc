{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    "stage-1",
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "loose": true,
      "modules": false
    }]
  ],
  "plugins": [
    "lodash",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "babel-plugin-transform-class-properties",
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-async-to-generator"
  ]
}
