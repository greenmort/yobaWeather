{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
      "linebreak-style": 0,
      "no-unused-vars": 0,
      "func-names": 0,
      "no-console": 0,
      "no-debugger": 0,
      "quotes": 1,
      "react/prop-types": 0,
      "jsx-a11y/anchor-is-valid":0,
      "react/default-props-match-prop-types": 0,
      "react/no-typos": 0,
      "react/prefer-stateless-function": 0,
      "react/no-unused-state": 0,
      "no-unreachable": 1
    }
  }